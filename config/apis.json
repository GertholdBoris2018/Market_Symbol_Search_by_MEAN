{
    "apiDomain":"https://www.cryptocompare.com",
    "getCoins": {
        "method": "GET",
        "url": "https://www.cryptocompare.com/api/data/coinlist/",
        "headers": {
            "Content-Type": "application/json"
        }
    },
    "getTickerCoin":{
        "method": "GET",
        "url": "https://api.coinmarketcap.com/v1/ticker/?start=0&limit=0",
        "headers": {
            "Content-Type": "application/json"
        }
    },
    "getCoinGlobal":{
        "method": "GET",
        "url": "https://files.coinmarketcap.com/generated/stats/global.json",
        "headers": {
            "Content-Type": "application/json"
        }
    },
    "getBitScreeners":{
        "method": "GET",
        "url": "https://api.bitscreener.com/api/v1/coins",
        "headers": {
            "Content-Type": "application/json"
        }
    }
}